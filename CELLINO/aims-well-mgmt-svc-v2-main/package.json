{
  "name": "aims-well-mgmt-svc-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "local": "env-cmd -f .env.local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "tap --node-arg=--require=ts-node/register --no-timeout --coverage-report=lcov --coverage-report=text src/**/*.spec.ts --no-browser",
    "generate:migrations": "env-cmd -f .env typeorm migration:generate src/migrations/Migration -d dist/typeorm-cli.config --pretty",
    "run:migrations": "typeorm migration:run -d dist/typeorm-cli.config"
  },
  "dependencies": {
    "@fastify/formbody": "^7.4.0",
    "@fastify/static": "^6.8.0",
    "@fastify/swagger": "^8.3.0",
    "@google-cloud/pubsub": "^3.3.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/lodash.merge": "^4.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "env-cmd": "^10.1.0",
    "fast-json-patch": "^3.1.1",
    "fastify": "^4.1.0",
    "fastify-swagger": "^5.2.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/tap": "^15.0.7",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tap": "^16.3.0",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "ts-node": {
    "files": true
  },
  "tap": {
    "branches": 70,
    "functions": 85,
    "lines": 85,
    "statements": 85,
    "nyc-arg": [
      "--exclude=**/*.spec.ts",
      "--exclude=**/enums/**",
      "--exclude=**/entities/**",
      "--exclude=**/dto/**"
    ]
  },
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowJs": false
  }
}
